{
    "compilerOptions": {
      "outDir": "./build/",
      "noImplicitAny": true,
      "module": "es6",
      "moduleResolution": "node",
      "target": "es5",
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "lib":[
          "es2015", "dom", "ES5", "ES6","DOM.Iterable","ES2015.Iterable"
      ]
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}